              "in": [
                {
                  "name": "clk"
                },
                {
                  "name": "addr2",
                  "range": "[7:0]",
                  "size": 8
                },
                {
                  "name": "data_in",
                  "range": "[23:0]",
                  "size": 24
                },
                {
                  "name": "write"
                },
                {
                  "name": "addr1",
                  "range": "[7:0]",
                  "size": 8
                }
              ],
              "out": [
                {
                  "name": "data",
                  "range": "[23:0]",
                  "size": 24
                }
              ]
            }
          },
          "position": {
